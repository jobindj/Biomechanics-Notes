@article{phan2021,
  title = {In {{Silico Finite Element Analysis}} of the {{Foot Ankle Complex Biomechanics}}: {{A Literature Review}}},
  shorttitle = {In {{Silico Finite Element Analysis}} of the {{Foot Ankle Complex Biomechanics}}},
  author = {Phan, P. K. and Vo, A. T. N. and Bakhtiarydavijani, A. and Burch, R. and Smith, B. and Ball, J. E. and Chander, H. and Knight, A. and Prabhu, R. K.},
  year = {2021},
  month = jun,
  journal = {Journal of Biomechanical Engineering},
  volume = {143},
  number = {9},
  issn = {0148-0731},
  doi = {10.1115/1.4050667},
  abstract = {Computational approaches, especially finite element analysis (FEA), have been rapidly growing in both academia and industry during the last few decades. FEA serves as a powerful and efficient approach for simulating real-life experiments, including industrial product development, machine design, and biomedical research, particularly in biomechanics and biomaterials. Accordingly, FEA has been a ``go-to'' high biofidelic software tool to simulate and quantify the biomechanics of the foot\textendash ankle complex, as well as to predict the risk of foot and ankle injuries, which are one of the most common musculoskeletal injuries among physically active individuals. This paper provides a review of the in silico FEA of the foot\textendash ankle complex. First, a brief history of computational modeling methods and finite element (FE) simulations for foot\textendash ankle models is introduced. Second, a general approach to build an FE foot and ankle model is presented, including a detailed procedure to accurately construct, calibrate, verify, and validate an FE model in its appropriate simulation environment. Third, current applications, as well as future improvements of the foot and ankle FE models, especially in the biomedical field, are discussed. Finally, a conclusion is made on the efficiency and development of FEA as a computational approach in investigating the biomechanics of the foot\textendash ankle complex. Overall, this review integrates insightful information for biomedical engineers, medical professionals, and researchers to conduct more accurate research on the foot\textendash ankle FE models in the future.},
  file = {C\:\\Users\\jobinj\\Box\\WorkBox\\Files\\3_Areas\\Literature_Zotero\\Phan\\Phan_2021_In_Silico_Finite_Element_Analysis_of_the_Foot_Ankle_Complex_Biomechanics_-_A.pdf;C\:\\Users\\jobinj\\Zotero\\storage\\IF4WR89V\\In-Silico-Finite-Element-Analysis-of-the-Foot.html}
}

@article{pickering2021,
