  keywords = {Beams,Co-Kriging,Kriging,Latin hypercube sampling,Multi-fidelity,Uncertainty quantification},
  file = {C\:\\Users\\jobinj\\Zotero\\storage\\949SB5QY\\S0096300321000357.html}
}

