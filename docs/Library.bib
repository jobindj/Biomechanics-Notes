@article{moure-guardiola2020,
  title = {Evaluation of {{Combat Helmet Behavior}} under {{Blunt Impact}}},
  author = {{Moure-Guardiola}, Carlos and Rubio, Ignacio and {Antona-Makoshi}, Jacobo and Olmedo, {\'A}lvaro and Loya, Jos{\'e} Antonio and {Rodr{\'i}guez-Mill{\'a}n}, Marcos},
