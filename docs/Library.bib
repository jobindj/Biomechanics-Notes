  author = {Machino, Masaaki and Yukawa, Yasutsugu and Imagama, Shiro and Ito, Keigo and Katayama, Yoshito and Matsumoto, Tomohiro and Inoue, Taro and Ouchida, Jun and Tomita, Keisuke and Ishiguro, Naoki and Kato, Fumihiko},
  year = {2016},
