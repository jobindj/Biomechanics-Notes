  title = {Technique and Preliminary Findings for in Vivo Quantification of Brain Motion during Injurious Head Impacts},
  author = {Whyte, T. and Liu, J. and Chung, V. and McErlane, S. A. and Abebe, Z. A. and McInnes, K. A. and Wellington, C. L. and Cripton, P. A.},
