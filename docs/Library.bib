@article{sahoo2015,
  title = {Influence of Stiffness and Shape of Contact Surface on Skull Fractures and Biomechanical Metrics of the Human Head of Different Population Underlateral Impacts},
  author = {Sahoo, Debasis and Deck, Caroline and Yoganandan, Narayan and Willinger, R{\'e}my},
