@article{iraeus2020,
  title = {Generic Finite Element Models of Human Ribs, Developed and Validated for Stiffness and Strain Prediction \textendash{} {{To}} Be Used in Rib Fracture Risk Evaluation for the Human Population in Vehicle Crashes},
  author = {Iraeus, Johan and Brolin, Karin and Pipkorn, Bengt},
