}

@article{wu2019a,
  title = {Explicit {{Modeling}} of {{White Matter Axonal Fiber Tracts}} in a {{Finite Element Brain Model}}},
  author = {Wu, Taotao and Alshareef, Ahmed and Giudice, J. Sebastian and Panzer, Matthew B.},
  year = {2019},
  month = sep,
  journal = {Annals of Biomedical Engineering},
  volume = {47},
  number = {9},
  pages = {1908--1922},
  issn = {1573-9686},
  doi = {10.1007/s10439-019-02239-8},
  abstract = {Many human brain finite element (FE) models lack mesoscopic (\textasciitilde\,1~mm) white matter structures, which may limit their capability in predicting TBI and assessing tissue-based injury metrics such as axonal strain. This study investigated an embedded method to explicitly incorporate white matter axonal fibers into an existing 50th percentile male brain model. The white matter was decomposed into myelinated axon tracts and an isotropic ground substance that had similar material properties to gray matter. The axon tract bundles were derived from a population-based tractography template explicitly modeled using 1-D cable elements. The axonal fibers and ground substance material were implemented using hyper-viscoelastic constitutive models, which were calibrated using white and gray matter brain tissue material testing data available in the literature. Finally, the new axon-based model was extensively validated for brain-skull relative deformation under various loading conditions (n\,=\,17) and showed good biofidelity compared to other brain models. Through these analyses, we demonstrated the applicability of this method for incorporating axonal fiber tracts into an existing FE brain model. The axon-based model will be a useful tool for understanding the mechanisms of TBI, evaluating tissue-based injury metrics, and developing injury mitigation systems.},
  langid = {english},
  file = {C\:\\Users\\jobinj\\Box\\WorkBox\\Files\\3_Areas\\Literature_Zotero\\Wu\\Wu_2019_Explicit_Modeling_of_White_Matter_Axonal_Fiber_Tracts_in_a_Finite_Element_Brain.pdf}
}

