@article{moreira-gonzalez2006,
  title = {Calvarial {{Thickness}} and {{Its Relation}} to {{Cranial Bone Harvest}}},
  author = {{Moreira-Gonzalez}, Andrea and Papay, Francis E. and Zins, James E.},
