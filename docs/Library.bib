  keywords = {Biomechanics,Fetus,Injury,Model,Pregnant,Tissue,Uterus},
  file = {C\:\\Users\\jobinj\\Zotero\\storage\\8XWT7ELC\\S0021929012002199.html}
}

@article{margulies1990,
