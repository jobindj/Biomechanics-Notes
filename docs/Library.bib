@article{pipkorn2018,
  title = {Occupant {{Protection}} in {{Far-Side Impacts}}},
  author = {Pipkorn, B and Larsson, K and Rapela, D Perez and Markusic, C and Whitcomb, B and Ayyagari, M and Sunnev{\aa}ng, C},
  year = {2018},
