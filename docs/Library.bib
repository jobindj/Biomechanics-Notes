  keywords = {3D model generator,comparative anatomy,mandible anatomy,mesh morphing,PCA,proximal femur anatomy,stochastic bone models},
  file = {C\:\\Users\\jobinj\\Zotero\\storage\\VXJPVWHX\\htm.html}
}

